{
  "name": "@journifyio/react-native-sdk-plugin-moengage",
  "version": "0.0.1",
  "description": "MoEngage destination plugin for Journify React Native SDK",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "scripts": {
    "build": "bob build",
    "test": "jest",
    "typescript": "tsc --noEmit",
    "clean": "rimraf lib node_modules"
  },
  "keywords": [
    "react-native",
    "journify",
    "moengage",
    "analytics"
  ],
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "author": "Journify",
  "license": "MIT",
  "peerDependencies": {
    "@journifyio/react-native-sdk": "^1.10.0",
    "react-native-moengage": "^11.2.2"
  },
  "devDependencies": {
    "@journifyio/react-native-sdk": "^1.10.0",
    "@journifyio/react-native-sdk-sovran": "^1.9.0",
    "jest": "^29.7.0",
    "react-native-adjust": "^5.1.0",
    "react-native-builder-bob": "^0.23.1",
    "react-native-moengage": "^11.2.2",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "resolutions": {
    "@types/react": "^18.2.44"
  },
  "engines": {
    "node": ">=12"
  }
}
